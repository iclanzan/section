<!DOCTYPE html>
<html dir="ltr" lang="en">
<head>
  <meta charset="utf-8" />

  <title><%= title %></title>

  <meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
  <link href="http://fonts.googleapis.com/css?family=Source+Sans+Pro:400,700,400italic|Source+Code+Pro&amp;subset=latin,latin-ext" rel="stylesheet" />
  <link href="/style.css" rel="stylesheet" media="all" />

  <link rel="canonical" href="<%= rel %>" />
  <meta name="description" content="<%= description %>" />

  <% if (ga_uid) { %><script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
  ga('create', '<%= ga_uid %>', 'songpane.com');
  ga('send', 'pageview');
  </script><% } %>
</head>
<body>

  <main>
    <header>
      <% if (date) { %>
      <time datetime="<%= dateFormat(date, 'yyyy-mm-dd') %>"><%= dateFormat(date, 'mmmm d, yyyy') %></time>
      <% } %>
    </header>
    <%= body %>
    <% if (is('index')) { %>
    <hr />
    <nav>
        <% children.forEach(function(page) { %>
        <article>
          <% if (page.date) { %>
          <time datetime="<%= dateFormat(page.date, 'yyyy-mm-dd') %>"><%= dateFormat(page.date, 'mmmm d, yyyy') %></time>
          <% } %>
          <h3><a href="<%= page.rel %>"><%= page.title %></a></h3>
          <p><%= page.description %></p>
        </article>
        <% }) %>
    </nav>
    <% } %>
  </main>

  <section>
    <aside>
      <header>
      </header>
      <h2><a class="home" href="<%= root.rel %>" title="Home"><%= root.title %></a><% if ((ancestor = ancestors[1]) && ancestor.children) { %><small class="section"><a href="<%= ancestor.rel %>" title="Go to <%= ancestor.title %>"><%= ancestor.title %></a></small><% } %></h2>
      <nav>
        <% root.children.forEach(function(section) { if (section.children) { %>
          <h5><a href="<%= section.rel %>"><%= section.title %></a></h5>
        <% }}) %>
        <div>
        <% if (is('index')) children.forEach(function(item) { %>
          <% if (item.children) { if (page != root) { %>
          <h5><a href="<%= item.rel %>"><%= item.title %></a></h5>
          <ul>
            <% item.children.forEach(function(subitem) { %>
            <li>
              <% if (subitem.date) { %>
              <time datetime="<%= dateFormat(subitem.date, 'yyyy-mm-dd') %>"><%= dateFormat(subitem.date, 'mmmm d, yyyy') %></time>
              <% } %>
              <a href="<%= subitem.rel %>"><%= subitem.title %></a>
            </li>
            <% }) %>
          </ul>
          <% }} else { if (!item.date) { %>
          <h6><a href="<%= item.rel %>"><%= item.title %></a></h6>
          <% }} %>
        <% }) %>
        </div>
        <% if (pages[0].date) { %>
        <h4>Recent</h4>
        <ul>
        <% for (var i = 0; i < 3; i++) if (pages[i].date) { %>
        <li>
          <time datetime="<%= dateFormat(pages[i].date, 'yyyy-mm-dd') %>"><%= dateFormat(pages[i].date, 'mmmm d, yyyy') %></time>
          <a href="<%= pages[i].rel %>"><%= pages[i].title %></a>
        </li>
        <% } %>
        </ul>
        <% } %>
      </nav>
      <footer>Generated with <a title="<%=pkg.name + ' v' + pkg.version%>" href="<%=pkg.homepage%>"><strong><%=pkg.title%></strong></a></footer>
    </aside>
  </section>
</body>
</html>
